<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/border_stroke">
        <shape android:shape="rectangle">
            <solid android:color="@color/view_not_selected"/>
            <padding android:bottom="@dimen/normal_border"
                android:top="@dimen/normal_border"
                android:right="@dimen/normal_border"
                android:left="@dimen/normal_border"/>
        </shape>
    </item>

    <item android:id="@+id/background_color">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
</layer-list>